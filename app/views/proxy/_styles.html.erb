<style>
/* rating input styles */

.rate {
    height: 46px;
    display: flex;
    transform: rotate(360deg) scaleX(-1);
    justify-content: flex-end;
}
.rate:not(:checked) > input {
    display: none;
}
.rate:not(:checked) > label {
    width:1em;
    overflow:hidden;
    white-space:nowrap;
    cursor:pointer;
    font-size:30px;
    color:#ccc;
}
.rate:not(:checked) > label:before {
    content: 'â˜… ';
}
.rate > input:checked ~ label {
    color: <%= @shop.star_color %>;    
}
.rate:not(:checked) > label:hover,
.rate:not(:checked) > label:hover ~ label {
    color: <%= @shop.star_color %>;   
}
.rate > input:checked + label:hover,
.rate > input:checked + label:hover ~ label,
.rate > input:checked ~ label:hover,
.rate > input:checked ~ label:hover ~ label,
.rate > label:hover ~ input:checked ~ label {
    color: <%= @shop.star_color %>;
}

/* review submitted container styles */

#ue-review-submitted-container > div {
  min-height: 200px;
  display: flex;
  justify-content: center;
  align-items: center;
}

#ue-review-submitted-container > div > span {
  font-weight: 600;
  font-size: large;
  color:#1cc286;
}


#ue-review-form-container{
  display:none;
}

.ue-show {
  display: block !important;
}
.pr-5 {
  padding-right: 5px !important;
}

.pr-10 {
  padding-right: 10px !important;
}

.pr-20 {
  padding-right: 20px !important;
}

.pr-30 {
  padding-right: 30px !important;
}

.pl-5 {
  padding-left: 5px !important;
}

.pl-10 {
  padding-left: 10px !important;
}

.pl-20 {
  padding-left: 20px !important;
}

.pl-30 {
  padding-left: 30px !important;
}

.row{display:-ms-flexbox;display:flex;-ms-flex-wrap:wrap;flex-wrap:wrap;margin-right:-15px;margin-left:-15px}

/* styles for x button to close modal */
.ue-close-modal {
  width: 40px;
  height: 40px;
  position: relative;
  margin-left: auto;
  cursor: pointer;
}
.ue-close-modal:after {
  content: '';
  height: 40px;
  border-left: 2px solid #000;
  position: absolute;
  transform: rotate(45deg);
  left: 24px;
}

.ue-close-modal:before {
  content: '';
  height: 40px;
  border-left: 2px solid #000;
  position: absolute;
  transform: rotate(-45deg);
  left: 24px;
}
/* end of styles for x button to close modal */

.ue-btn-close{
  width:32px;
  height:32px;
  display:block;
  float:right;
}


.ue-btn-close::before{
  margin-top:12px;
  -webkit-transform:rotate(45deg);
  -moz-transform:rotate(45deg);
  -o-transform:rotate(45deg);
  transform:rotate(45deg);
}

.ue-btn-close::after{
  margin-top:-6px;
  -webkit-transform:rotate(-45deg);
  -moz-transform:rotate(-45deg);
  -o-transform:rotate(-45deg);
  transform:rotate(-45deg);
}

.ue-content{
  padding:2%;
}

/* End of styles for modal */

/* Start of styles for slideshow */

/* Slideshow container */
.slideshow-container {
  max-width: 1000px;
  position: relative;
  margin: auto;
}

/* Hide the images by default */
.mySlides {
  display: none;
}

/* Next & previous buttons */
.next {
  cursor: pointer;
  position: absolute;
  top: 50%;
  width: auto;
  right: -80px;
  margin-top: -22px;
  padding: 16px;
  color: white;
  font-size: 40px;
  user-select: none;
}

.prev {
  cursor: pointer;
  position: absolute;
  top: 50%;
  width: auto;
  left: -80px;
  margin-top: -22px;
  padding: 16px;
  color: white;
  font-size: 40px;
  user-select: none;
}

/* Position the "next button" to the right */
.next {
  
  border-radius: 3px 0 0 3px;
}

/* On hover, add a black background color with a little bit see-through */
.prev:hover, .next:hover {
  color: #fff !important;
}

/* Number text (1/3 etc) */
.numbertext {
  color: #f2f2f2;
  font-size: 12px;
  padding: 8px 12px;
  position: absolute;
  top: 0;
}

/* The dots/bullets/indicators */
.dot {
  cursor: pointer;
  height: 15px;
  width: 15px;
  margin: 0 2px;
  background-color: #bbb;
  border-radius: 50%;
  display: inline-block;
  transition: background-color 0.6s ease;
}

.active, .dot:hover {
  background-color: #717171;
}

/* Fading animation */
.fade {
  -webkit-animation-name: fade;
  -webkit-animation-duration: 1.5s;
  animation-name: fade;
  animation-duration: 1.5s;
}

@-webkit-keyframes fade {
  from {opacity: .4}
  to {opacity: 1}
}

@keyframes fade {
  from {opacity: .4}
  to {opacity: 1}
}
/* End of styles for slideshow */
.review-photo {
  cursor: pointer;
} 


.ue-pagination {
  padding: 3px;
  margin: 3px;
}
.ue-pagination a {
  margin: 2px;
  text-decoration: none;
}
.ue-pagination a:hover, .ue-pagination a:active {
  margin: 2px;
  color: #000;
}
.ue-pagination span.current {
  font-weight: bold;
  color: #FFF;
}
em.current {
  margin: 2px;
  font-weight: 600;
  font-style: inherit;
}
.ue-pagination span.disabled {
  margin: 2px;
  color: #ddd;
}

/* start of rating */

.ue-rating {
  display: inline-block;
  unicode-bidi: bidi-override;
  color: #efefef;
  font-size: 25px;
  height: 25px;
  width: auto;
  margin: 0;
  position: relative;
  padding: 0;
}

.ue-rating-upper {
  color: <%= @shop.star_color %>;
  padding: 0;
  position: absolute;
  z-index: 1;
  display: flex;
  top: 0;
  left: 0;
  overflow: hidden;
}

.ue-rating-lower {
  padding: 0;
  display: flex;
  z-index: 0;
}

/* End of style for rating stars */


/* start of small rating */

.ue-small-rating {
  display: inline-block;
  unicode-bidi: bidi-override;
  color: #efefef;
  font-size: 20px;
  height: 20px;
  width: auto;
  margin: 0;
  position: relative;
  padding: 0;
}

.big-median {
  font-size: 40px;
  font-weight: 600;
}

.ue-review {
  border: 1px solid #eee;
  margin-bottom: 30px;
  padding-top: 25px;
  padding: 24px;
  background-color: #fafafa;
}

.ue-review-content {
  margin: 0 0 15px;
  clear: both;
}
/* End of style for small rating stars */

/* SO review images modal */

.ue-review-images-modal {
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 999;

  pointer-events: none;
  transition: all 0.3s;
  visibility: visible;
  opacity: 1;
  pointer-events: auto;
  display: inherit !important;
}
/* change this name */
.visible {

}

.ue-review-images-modal > div {
  width: 60%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  padding: 2em;
  background: #fff;
}
.ue-review-images-modal header {
  font-weight: bold;
}
.ue-review-images-modal h1 {
  font-size: 150%;
  margin: 0 0 15px;
}

.modal-close {
  color: #aaa;
  line-height: 50px;
  font-size: 80%;
  position: absolute;
  right: 0;
  text-align: center;
  top: 0;
  width: 70px;
  text-decoration: none;
}
.modal-close:hover {
  color: black;
}

.ue-review-images-modal div:not(:last-of-type) {
